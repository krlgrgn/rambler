<!DOCTYPE html>
<html>
  <head>
    <title>Quest</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <nav class="navbar navbar-inverse navbar-static-top" role="navigation">
      <%= render "layouts/header" %>
    </nav>
    <div id="content">
      <div class="container">
        <%= yield %>
      </div>
    </div>
    <div>
      <%= debug(params) if Rails.env.development? %>
    </div>
    <%= javascript_include_tag "application" %>
  </body>
</html>
